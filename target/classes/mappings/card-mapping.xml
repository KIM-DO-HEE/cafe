<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="cardDAO">

	<!-- userId를 파라미터값으로 넘기는 방법 생각해보기 -->
	<insert id="insertMenu" parameterType="cardVO">
		INSERT INTO card(userId,cardName,cardPw)
		VALUES(#{userId},#{cardName},#{cardPw})
	</insert>

</mapper>
